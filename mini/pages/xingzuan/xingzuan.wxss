/* pages/xingzuan/xingzuan.wxss */
.xingzuanBody{
  width:670rpx;
  margin:30rpx auto;
  font-family: PingFangSC-Medium;
}
.xingzuanjilu{
  display: flex;
  align-items: center;
  padding-left:60rpx;
  width:100%;
  height:240rpx;
    background-image: url(https://cms-img.oss-cn-hangzhou.aliyuncs.com/wechat/planet/v2/jilubg.png);
  background-size: 670rpx 240rpx;
}
.left{
  width:60%;
  color: #FFFFFF;
  font-size: 28rpx;
letter-spacing: 1.56rpx;
}
.left view{
  height:90rpx;
  font-size: 64rpx;

letter-spacing: 3.56rpx;
}
.right{
  width:40%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.right button{
  margin-right:50rpx;
  background: transparent;
  padding: 0;
  width:87rpx;
  height:50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: white;
  border: 1rpx solid #FFFFFF;
  border-radius: 8rpx;
}
.xingzuanNav{
  margin-top:40rpx;
}
.xingzuanHead{
  font-size: 42rpx;
color: #7F00EE;
font-weight: 600;
letter-spacing: 2.33rpx;
}
.xingzuanCon{
    margin-top:30rpx;
}
.xingzuanCon view{
  margin-bottom:40rpx;
  font-size: 32rpx;
color: #333333;
letter-spacing: 3rpx;
line-height: 52rpx;
}
.xingzuanshouzhiHead{
  font-size: 42rpx;
color: #7F00EE;
font-weight:600;
letter-spacing: 2.33rpx;
}