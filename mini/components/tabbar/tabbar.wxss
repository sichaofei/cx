/* components/tabbar/tabbar.wxss */
.tabbar{
  width:100%;
  height: 98rpx;
  background:#FFF;
  font-size:20rpx;
  color:#CCCCCC;
  box-shadow: 0 -2rpx 6rpx 0 #EFEFEF;
  display: flex;
  position: fixed;
  bottom: 0;
}
.tabbar-item{
  text-align: center;
  overflow: hidden;
  flex: 1;
}
.tabbar-item .selected{
  color:#1BC787;
}
.icon{
  width:50rpx;
  height: 52rpx;
}